namespace langchain {
    string get_version();
    
    [Throws=LangChainError]
    string invoke_llm(string model_name, string prompt);
};

[Error]
enum LangChainError {
    "InvalidModel",
    "ApiError",
    "NetworkError", 
    "InvalidArgument",
    "TokenLimitExceeded",
    "Unauthorized",
    "ServerError",
    "UnknownError"
};

dictionary Message {
    string content;
    string message_type;
};
